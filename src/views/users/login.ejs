<!DOCTYPE html>
<html lang="en">

    <%- include('../partials/head.ejs') %> 

<body>

    <%- include('../partials/header.ejs') %> 

    <main>

        <section class="sectionLogin">

            <h1 class="titular-login">Inicia Sesion:</h1>

            <form class="formlogin" action="#" method="post">

                <label for="email">Email:</label>
                <input class="input-form" type="email" name="usuario-email" placeholder="usuario@gmail.com" id="email" required>

                <label for="contraseña">Contraseña:</label>
                <input class="input-form"  type="password" name="usuario-contraseña" id="contraseña" required>

                <label class="recuerdame"><input type="checkbox" class="recuerdamelogin">Recordarme</label>

                <Button type="submit" class="button">Continuar</Button>

                <a class="texto-enlace" href="#">¿Olvidaste tu Contraseña?</a>

                <a class="texto-enlace" href="register">¡No tenes cuenta todavia?</a>

            </form>

        </section>

    </main>

    <%- include('../partials/footer.ejs') %> 

</body>

</html>